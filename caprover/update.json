{
    "appName": "site-store-db",
    "instanceCount": 1,
    "notExposeAsWebApp": true,
    "envVars": [
        {
            "key": "POSTGRES_USER",
            "value": "$POSTGRES_USER"
        },
        {
            "key": "POSTGRES_PASSWORD",
            "value": "$POSTGRES_PASSWORD"
        },
        {
            "key": "POSTGRES_DB",
            "value": "$POSTGRES_DB"
        }
    ]
}