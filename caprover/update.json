{
    "appName": "fhir-store-db",
    "instanceCount": 1,
    "notExposeAsWebApp": true,
    "envVars": [
        {
            "key": "CODA_FHIR_STORE_DB_USERNAME",
            "value": "$CODA_FHIR_STORE_DB_USERNAME"
        },
        {
            "key": "CODA_FHIR_STORE_DB_PASSWORD",
            "value": "$CODA_FHIR_STORE_DB_PASSWORD"
        },
        {
            "key": "CODA_FHIR_STORE_DB_NAME",
            "value": "$CODA_FHIR_STORE_DB_NAME"
        }
    ]
}